</head>
<body>
<%- include('./includes/header.ejs') %>
<br>
<div class="row row-cols-1 row-cols-md-4">
    <% Items.forEach(function(item){ %>
    <div class="container" >
      <div class="col mb-2">
        <div class="polaroid">
          <div class="card bg-light">
              <div class="card-body">
                <h4 class="card-title" style="font-family: sans-serif; font-weight: bold;"><%= item.foodItemName %></h5>
                <p class="card-text" style="font-family: sans-serif; font-weight: bold;"> orderID: <%= item.id %></p>
                <p class="card-text" style="font-family: sans-serif; font-weight: bold;">By - <%= item.customerName %></p>
              </div>
          </div>
        </div>
      </div>
    </div>
    <% }); %>
</div>
<%- include('./includes/footer.ejs') %>